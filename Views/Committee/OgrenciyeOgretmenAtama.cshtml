@{
    ViewBag.Title = "Komisyon Portalı";
    Layout = "~/Views/Shared/_LayoutDashboardCommittee.cshtml";
}

<div class="col-md-12">
    <div class="card">
        <div class="card-header card-header-rose card-header-icon">
            <div class="card-icon">
                <i class="material-icons">assignment</i>
            </div>
            <h4 class="card-title">Notlandırılan Öğrenciler</h4>
        </div>
        <div class="card-body">
            <div class="material-datatables">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th class="text-center">#</th>
                            <th>İsim - Soy İsim</th>
                            <th>Numara</th>
                            <th>Öğretmen Ata</th>
                        </tr>
                    </thead>
                    <tbody>

                        <tr>
                            <td class="text-center">#id</td>
                            <td>isim soyisim</td>
                            <td>111111</td>
                            <td>
                                <a type="button" rel="tooltip" id="#" class="btn btn-link btn-success btn-just-icon remove" data-toggle="tooltip" title="Öğretmen Ata" href="#">
                                    <i class="material-icons">noteadd</i>
                                </a>
                            </td>
                        </tr>

                    </tbody>

                </table>
            </div>
        </div>
    </div>
</div>
