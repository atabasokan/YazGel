@{
    ViewBag.Title = "Öğretmen Portalı";
    Layout = "~/Views/Shared/_LayoutDashboardTeacher.cshtml";
}

<div class="col-md-12">
    <div class="card">
        <div class="card-header card-header-rose card-header-icon">
            <div class="card-icon">
                <i class="material-icons">assignment</i>
            </div>
            <h4 class="card-title">Öğrenciler</h4>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th class="text-center">#</th>
                            <th>İsim - Soy İsim</th>
                            <th>Numara</th>
                            <th>Şifre</th>
                        </tr>
                    </thead>

                </table>
            </div>
        </div>
    </div>
</div>